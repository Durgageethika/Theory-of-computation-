#include<stdio.h>
#include<string.h>
#define MAX 100
int main()
{
	char str[MAX],f='a';
	int i;
	printf("enter the string to be checked:");
	scanf("%s",str);
	int length=strlen(str);
	if (str[0]==str[length-1]&&str[1]==str[length-2])
	{
		printf("accepted");
	}
	else
	{
		printf("rejected");
		return 0;
	}
}
