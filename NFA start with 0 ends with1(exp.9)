#include<stdio.h>
#include<string.h>
int main()
{
	char s[10],B='0',A='1';
	int n,c=0;
	printf("enter the string");
	scanf("%s",s);
	for(int i=0;i<n;i++)
	{
		if(s[i]=='0'||s[i]=='1')
		{
			c=0;
		}
		else
		{
			c='1';
			printf("invalid");
			break;
		}
	}
	if(c==0)
	{
		if(s[0]==B&&s[n-1]==A)
		{
			printf("the string was accepted");
		}
		else
		{
			printf("not NFA");
		}
	}
	
}
